{
  "name": "pokechess",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=packages/web",
    "build": "npm run build --workspace=packages/web",
    "preview": "npm run preview --workspace=packages/web",
    "anchor:build": "cd packages/anchor && anchor build",
    "anchor:deploy": "cd packages/anchor && anchor deploy",
    "anchor:test": "cd packages/anchor && anchor test"
  }
}
